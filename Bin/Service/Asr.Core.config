<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <engine>
    <baidu>
      <!-- 百度支持语种：普通话、英语、粤语、四川话。-->
      <appId>15499012</appId>
      <apiKey>cFSLHg4rW9N6x4Sc9NZ4dSvc</apiKey>
      <secretKey>301ORzSxeLoagyrNb4qElEMLzeGbMbG6</secretKey>
    </baidu>
    
    <jths>
      <!-- 捷通华声支持语种：英语、普通话、维吾尔语、国语（台湾）、粤语、哈萨克语、藏语、朝鲜语、彝语、蒙文、藏语安多、藏语康巴、广东阳江话、壮语、闽南语、上海话、四川话。-->
      <appKey>995d54de</appKey>
      <developerKey>889f9c04b9a7dc6c531c0f92dd3834cb</developerKey>
      <cloudUrl>api.hcicloud.com:8888</cloudUrl>
      <authPath>Log</authPath>
      <logFilePath>Log</logFilePath>
      <logLevel>5</logLevel>
      <logFileSize>512</logFileSize>
    </jths>
    
    <ifly>
      <!-- 讯飞支持语种：普通话、英语、粤语、四川话、东北话、河南话、天津话、山东话、贵州话、宁夏话、云南话、陕西话、甘肃话、武汉话、河北话、合肥话、长沙话、上海话、太原话。-->
      <appId name="应用ID">5c344ac9</appId>
    </ifly>
    
    <mzywfy>
      <!--汉文、蒙古文、藏文、维吾尔文、哈萨克文、朝鲜文、彝文、壮文-->
      <url>http://www.mzywfy.org.cn/ajaxservlet</url>
      <referer>http://www.mzywfy.org.cn/translate.jsp</referer>
      <timeout>5000</timeout>
    </mzywfy>
  </engine>

  <language>
    <!-- name 是在代码中的定义 -->
    <!-- text 是在界面上显示的文本 -->
    <!-- capacity 表示哪些 SDK 具备该语种的识别能力 -->
    <!-- engine 表示该语种当前使用的 SDK -->
    <!-- valid 表示当前系统是否开放该语种的识别能力 -->
    <!-- 注意：在修改此配置时，只需要修改 engine 和 valid 字段的值，其他字段不能修改。-->
    <item name="English" text="英语" capacity="baidu、jths、ifly" engine="ifly" valid ="true"></item>
    <item name="Mandarin" text="普通话" capacity="baidu、jths、ifly" engine="jths" valid ="true"></item>
    <item name="Yue" text="粤语" capacity="baidu、jths、ifly" engine="baidu" valid ="false"></item>
    <item name="Sichuan" text="四川话" capacity="baidu、jths、ifly" engine="baidu" valid ="true"></item>
    <item name="Uygur" text="维吾尔语" capacity="jths" engine="jths" valid ="true"></item>
    <item name="Utsang" text="藏语" capacity="jths" engine="jths" valid ="true"></item>
    <item name="Amdo" text="藏语安多" capacity="jths" engine="jths" valid ="false"></item>
    <item name="Kham" text="藏语康巴" capacity="jths" engine="jths" valid ="false"></item>
    <item name="Taiwan" text="国语(台湾)" capacity="jths" engine="jths" valid ="false"></item>
    <item name="Kazak" text="哈萨克语" capacity="jths" engine="jths" valid ="false"></item>
    <item name="Korean" text="朝鲜语" capacity="jths" engine="jths" valid ="false"></item>
    <item name="Yi" text="彝语" capacity="jths" engine="jths" valid ="false"></item>
    <item name="Mongolian" text="蒙文" capacity="jths" engine="jths" valid ="false"></item>
    <item name="Yangjiang" text="广东阳江话" capacity="jths" engine="jths" valid ="false"></item>
    <item name="Zhuang" text="壮语" capacity="jths" engine="jths" valid ="false"></item>
    <item name="Minnan" text="闽南语" capacity="jths" engine="jths" valid ="false"></item>
    <item name="Shanghai" text="上海话" capacity="jths、ifly" engine="jths" valid ="false"></item>
    <item name="Dongbei" text="东北话" capacity="ifly" engine="ifly" valid ="false"></item>
    <item name="Henan" text="河南话" capacity="ifly" engine="ifly" valid ="false"></item>
    <item name="Tianjin" text="天津话" capacity="ifly" engine="ifly" valid ="false"></item>
    <item name="Shandong" text="山东话" capacity="ifly" engine="ifly" valid ="false"></item>
    <item name="Guizhou" text="贵州话" capacity="ifly" engine="ifly" valid ="false"></item>
    <item name="Ningxia" text="宁夏话" capacity="ifly" engine="ifly" valid ="false"></item>
    <item name="Yunnan" text="云南话" capacity="ifly" engine="ifly" valid ="false"></item>
    <item name="Shanxi" text="陕西话" capacity="ifly" engine="ifly" valid ="false"></item>
    <item name="Gansu" text="甘肃话" capacity="ifly" engine="ifly" valid ="false"></item>
    <item name="Wuhan" text="武汉话" capacity="ifly" engine="ifly" valid ="true"></item>
    <item name="Hebei" text="河北话" capacity="ifly" engine="ifly" valid ="false"></item>
    <item name="Hefei" text="合肥话" capacity="ifly" engine="ifly" valid ="false"></item>
    <item name="Changsha" text="长沙话" capacity="ifly" engine="ifly" valid ="false"></item>
    <item name="Taiyuan" text="太原话" capacity="ifly" engine="ifly" valid ="false"></item>
  </language>

  <translate>
    <!--捷通华声（jths）：英语、维语-->
    <!--民族语文翻译局（mzywfy）：汉文、蒙古文、藏文、维吾尔文、哈萨克文、朝鲜文、彝文、壮文-->
    <!--小牛翻译：英语（niu）、粤语、朝鲜语、蒙语、藏语，等其他国外语种-->
    <item name="English" text="英语" capacity="jths、niu" engine="niu" valid ="true"></item>
    <item name="Uygur" text="维吾尔语" capacity="jths、mzywfy" engine="mzywfy" valid ="true"></item>
    <item name="Mongolian" text="蒙文" capacity="mzywfy、niu" engine="mzywfy" valid ="true"></item>
    <item name="Utsang" text="藏语" capacity="mzywfy、niu" engine="niu" valid ="true"></item>
    <item name="Kazak" text="哈萨克语" capacity="mzywfy" engine="mzywfy" valid ="true"></item>
    <item name="Korean" text="朝鲜语" capacity="mzywfy、niu" engine="mzywfy" valid ="true"></item>
    <item name="Yi" text="彝语" capacity="mzywfy" engine="mzywfy" valid ="true"></item>
    <item name="Zhuang" text="壮语" capacity="mzywfy" engine="mzywfy" valid ="true"></item>
  </translate>

</configuration>